  <div class="span8 help_img">
  <h5>Step.3</h5>
    <%= image_tag 'step/step03.jpg' %>
  </div>

  <div class="span4 help_script">
	<p>Click [Create]</p>
	<hr>
	<p>「Create」ボタンをクリック</p>
  </div>

</div>

<div class="row-fluid">
  <div class="span8 help_img">
  <h5>Step.4</h5>
    <%= image_tag 'step/step04.jpg' %>
  </div>

  <div class="span4 help_script">
		<p>Your 3D model will be displayed</p>
